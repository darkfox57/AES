{
  "text": [
    {
      "label": "Nombre y apellidos",
      "type": "text",
      "name": "nombre",
      "required": true,
      "pattern": "^[A-Za-zÁ-ÿ\\s]+$"
    },
    {
      "label": "Correo de contacto",
      "type": "text",
      "name": "correo",
      "required": true,
      "pattern": "^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$"
    },
    {
      "label": "Celular de contacto",
      "type": "number",
      "name": "celular",
      "required": true,
      "pattern": "^[0-9]+$",
      "maxLength": 15
    }
  ],

  "seleccion": [
    {
      "label": "Pais",
      "name": "pais",
      "type": "select",
      "required": true
    }
  ],
