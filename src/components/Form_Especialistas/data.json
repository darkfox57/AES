      "required": true
    },
    {
      "label": "Salud Mental",
      "type": "radio",
      "name": "cargo",
      "value": "mental",
      "required": true
    },
    {
      "label": "Salud Social",
      "type": "radio",
      "name": "cargo",
      "value": "social",
      "required": true
